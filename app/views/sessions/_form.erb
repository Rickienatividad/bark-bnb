<%= form_with method: "post", url: "/sessions" do |form|%>

  <div>
    <%= form.label :email%>
    <%= form.text_field :email%>
  </div>

  <div>
    <%= form.label :password%>
    <%= form.password_field :password%>
  </div>

  <div>
    <%= form.submit "Submit"%>
  </div>
  <%= link_to "Sign Up", "/signup"%>
 
<% end %>